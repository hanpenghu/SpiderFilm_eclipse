package hanhan.entity.MenDianTabEntity;
//门店库存表
public class Hishop_StoreStock {
	public Hishop_StoreStock() {}
//////////////////////////////////////////////////////////////////////////////
	int StoreId=0;//	int	门店id
	int ProductID=0;//	int	产品id
	String SkuId="doItForYourself";//	nvarchar(100)	规格id
	int Stock=0;//	int	库存
	int WarningStock=0;//	int	警戒库存
	int FreezeStock=0;//	int	
//////////////////////////////////////////////////////////////////////////////
	public int getStoreId() {
		return StoreId;
	}
	public void setStoreId(int storeId) {
		StoreId = storeId;
	}
	public int getProductID() {
		return ProductID;
	}
	public void setProductID(int productID) {
		ProductID = productID;
	}
	public String getSkuId() {
		return SkuId;
	}
	public void setSkuId(String skuId) {
		SkuId = skuId;
	}
	public int getStock() {
		return Stock;
	}
	public void setStock(int stock) {
		Stock = stock;
	}
	public int getWarningStock() {
		return WarningStock;
	}
	public void setWarningStock(int warningStock) {
		WarningStock = warningStock;
	}
	public int getFreezeStock() {
		return FreezeStock;
	}
	public void setFreezeStock(int freezeStock) {
		FreezeStock = freezeStock;
	}
//////////////////////////////////////////////////////////////////////////////
	@Override
	public String toString() {
		return "Hishop_StoreStock [StoreId=" + StoreId + ", ProductID=" + ProductID + ", SkuId=" + SkuId + ", Stock="
				+ Stock + ", WarningStock=" + WarningStock + ", FreezeStock=" + FreezeStock + "]";
	}
	
//////////////////////////////////////////////////////////////////////////////
}
//////////////////////////////////////////////////////////////////////////////